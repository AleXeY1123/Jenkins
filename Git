#!groovy
pipeline {
    agent any

    stages {
        stage('==========Checkout1==========') {
            steps {
                checkout([$class: 'GitSCM', branches: [[name: '*/main']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[credentialsId: 'root_gitlab', url: 'http://10.0.2.5:80/root/test123.git']]])
                sh "ls -lart ./*"
              }
            }
        stage('==========Checkout2==========') {
            steps {
                checkout([$class: 'GitSCM', branches: [[name: '*/main']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[credentialsId: 'my_key_github_ssh_username_with_pk', url: 'git@10.0.2.5:root/test123.git']]])
                sh "ls -lart ./*"
              }
            }
       // stage('==========Git connect==========') {
