#!groovy
pipeline {
    agent any
    stages {
        stage('===Test===') {
            steps {
                writeFile file: 'file.xml', text: 'text text text text text text text text text'
                sh 'cat file.xml'
            }
        }
    }
    post {
        always {
            archiveArtifacts artifacts: 'file.xml', fingerprint: true
        }
    }
}
